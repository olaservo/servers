    "watch": "tsc --watch",
    "start:stdio": "node dist/index.js stdio",
    "start:sse": "node dist/index.js sse",
    "start:streamableHttp": "node dist/index.js streamableHttp",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "npm:@olaservo/mcp-sdk@1.25.2-sep834.4",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "jszip": "^3.10.1",
    "zod": "^3.25.0",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "shx": "^0.3.4",
    "typescript": "^5.6.2",
    "prettier": "^2.8.8"
  }
