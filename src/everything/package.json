{
  "name": "@olaservo/mcp-server-everything-sep834",
  "version": "1.1.0",
  "description": "MCP server demonstrating SEP-834 JSON Schema 2020-12 support (arrays, primitives, compositions)",
  "license": "MIT",
  "author": "Ola Hungerford (@olaservo)",
  "homepage": "https://github.com/olaservo/servers/tree/sep-834-json-schema-2020-12/src/everything",
  "bugs": "https://github.com/olaservo/servers/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/olaservo/servers.git"
  },
  "type": "module",
  "bin": {
    "mcp-server-everything": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && shx cp -r docs dist/ && shx chmod +x dist/*.js",
